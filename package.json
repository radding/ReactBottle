{
  "name": "react-bottle",
  "version": "0.1.0",
  "description": "A di container for react",
  "main": "index.js",
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"__tests__/**/*.ts\" \"__tests__/**/*.tsx\"",
    "lint:fix": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"__tests__/**/*.ts\" \"__tests__/**/*.tsx\" --fix",
    "test": "jest --config jest.config.json",
    "test:ci": "jest --config jest.config.json --runInBand",
    "prepare": "npm run build",
    "build": "tsc --declaration",
    "clean": "rm dist/*"
  },
  "author": "Yoseph Radding",
  "license": "ISC",
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.13",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "bottlejs": "^1.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}