{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "__test__"
        ]
    },
    "jsRules": {},
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "object-literal-sort-keys": false,
        "forin": false,
        "quotemark": [
            true,
            "double"
        ],
        "ordered-imports": false
    },
    "rulesDirectory": []
}